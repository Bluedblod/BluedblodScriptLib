J.MouseButton1Click:Connect(function()
	print("hotkey activated")
	local character = plrrr.Character or plrrr.CharacterAdded:Wait()

	local function onEntered(otherPart)
		local range = 100 -- Change this value to adjust the range
		local otherHumanoid = otherPart.Parent:FindFirstChildOfClass("Humanoid")
		if otherHumanoid then
			local distance = (otherPart.Position - character.HumanoidRootPart.Position).Magnitude
			while distance <= range do
				wait(0.05)
				character:Click()
				print("clicked")
			end
		end
	end

	local function onPartAdded(part)
		local range = 10 -- Change this value to adjust the range
		if part:IsA("BasePart") and part.Name == "HumanoidRootPart" then
			part.Touched:Connect(function(otherPart)
				onEntered(otherPart, range)
			end)
		end
	end

	for _, part in pairs(character:GetDescendants()) do
		local range = 10
		if part:IsA("BasePart") and part.Name == "HumanoidRootPart" then
			part.Touched:Connect(function(otherPart)
				onEntered(otherPart, range)
			end)
		end
	end

	character.DescendantAdded:Connect(onPartAdded)
end)
