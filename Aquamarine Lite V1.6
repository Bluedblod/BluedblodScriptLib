local gui = AquamarineHub:WaitForChild("Client")
local dragging = false
local offsetX, offsetY = 0, 0

local function handleMouse(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		if input.UserInputState == Enum.UserInputState.Begin then
			local mousePos = input.Position
			if mousePos.X >= gui.AbsolutePosition.X and mousePos.X <= gui.AbsolutePosition.X + gui.AbsoluteSize.X and
				mousePos.Y >= gui.AbsolutePosition.Y and mousePos.Y <= gui.AbsolutePosition.Y + gui.AbsoluteSize.Y then
				dragging = true
				offsetX = mousePos.X - gui.AbsolutePosition.X
				offsetY = mousePos.Y - gui.AbsolutePosition.Y
			end
		elseif input.UserInputState == Enum.UserInputState.End then
			dragging = false
		end
	elseif input.UserInputType == Enum.UserInputType.MouseMovement and dragging then
		gui.Position = UDim2.new(0, input.Position.X - offsetX, 0, input.Position.Y - offsetY)
	end
end

gui.InputBegan:Connect(handleMouse)
gui.InputChanged:Connect(handleMouse)
gui.InputEnded:Connect(handleMouse)

